<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.chua</groupId>
    <artifactId>spring-support-parent-starter</artifactId>
    <version>4.0.0.36</version>
  </parent>
  <artifactId>spring-support-report-client-starter</artifactId>
  <version>4.0.0.36</version>
  <description>上报信息客户端模块</description>
  <licenses>
    <license>
      <name>Apache License, Version 2.0</name>
      <url>https://www.apache.org/licenses/LICENSE-2.0</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <properties>
    <jna-version>5.14.0</jna-version>
    <maven.compiler.target>25</maven.compiler.target>
    <maven.compiler.source>25</maven.compiler.source>
    <docker-java-version>3.4.0</docker-java-version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <hutool.version>5.8.34</hutool.version>
  </properties>
  <dependencies>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>spring-support-ssh-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>org.apache.commons</groupId>
      <artifactId>commons-exec</artifactId>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>spring-support-oauth-client-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>com.alibaba.fastjson2</groupId>
      <artifactId>fastjson2</artifactId>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>spring-support-common-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>spring-support-job-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>io.micrometer</groupId>
      <artifactId>micrometer-registry-prometheus</artifactId>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>utils-support-oshi-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>utils-support-groovy-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>spring-support-swagger-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>utils-support-vertx-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>spring-support-discovery-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>org.apache.commons</groupId>
      <artifactId>commons-compress</artifactId>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>com.chua</groupId>
      <artifactId>spring-support-sync-starter</artifactId>
    </dependency>
    <dependency>
      <groupId>jakarta.servlet</groupId>
      <artifactId>jakarta.servlet-api</artifactId>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>${maven-compiler-plugin.version}</version>
        <configuration>
          <source>25</source>
          <target>25</target>
          <release>25</release>
          <encoding>UTF-8</encoding>
          <useIncrementalCompilation>true</useIncrementalCompilation>
          <compilerArgs>
            <arg>-parameters</arg>
            <arg>-J--enable-native-access=ALL-UNNAMED</arg>
          </compilerArgs>
          <annotationProcessorPaths>
            <path>
              <groupId>org.projectlombok</groupId>
              <artifactId>lombok</artifactId>
            </path>
          </annotationProcessorPaths>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
