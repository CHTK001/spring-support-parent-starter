    <!-- 构建配置 -->
    <build>
        <plugins>
            <!-- Maven源码打包插件 - 生成源码jar包 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-source-plugin</artifactId>
                <version>${maven-source-plugin.version}</version>
                <executions>
                    <!-- 在package阶段生成源码jar包 - 已注释
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>jar-no-fork</goal>
                        </goals>
                    </execution>
                    -->
                    <!-- 在compile阶段生成源码jar包 -->
                    <execution>
                        <phase>compile</phase>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>

            <!-- Maven单元测试插件 - 配置测试执行 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>${maven-surefire-plugin.version}</version>
                <configuration>
                    <skip>true</skip> <!-- 跳过单元测试执行 -->
                </configuration>
            </plugin>
            <!-- Maven JavaDoc插件 - 生成API文档 (已注释)
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
                <version>${maven-javadoc-plugin.version}</version>
                <configuration>
                    <sourcepath>src/main/java</sourcepath>
                    <detectJavaApiLink>false</detectJavaApiLink>
                    <detectOfflineLinks>false</detectOfflineLinks>
                </configuration>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            -->

            <!-- Maven编译插件 - 配置Java编译参数 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>${maven-compiler-plugin.version}</version>
                <configuration>
                    <source>${java.version}</source>                    <!-- 源码Java版本 -->
                    <target>${java.version}</target>                    <!-- 目标Java版本 -->
                    <encoding>${project.build.sourceEncoding}</encoding> <!-- 编码格式 -->
                    <compilerArgs>
                        <arg>-parameters</arg>                          <!-- 保留参数名信息 -->
                    </compilerArgs>
                </configuration>
            </plugin>

            <!-- Maven发布插件 - 管理项目版本发布 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-release-plugin</artifactId>
                <version>3.0.1</version>
                <configuration>
                    <tagNameFormat>v@{project.version}</tagNameFormat>  <!-- Git标签格式 -->
                    <autoVersionSubmodules>true</autoVersionSubmodules> <!-- 自动更新子模块版本 -->
                </configuration>
            </plugin>

