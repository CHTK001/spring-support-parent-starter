    <artifactId>spring-support-parent-starter</artifactId>
    <packaging>pom</packaging> 
    <version>${revision}</version>
    
    <name>Spring Support Parent Starter</name>
    <description>Spring Boot 3.x企业级应用支持框架，提供模块化的功能组件和开箱即用的集成方案</description>
    <url>https://github.com/your-org/spring-support-parent-starter</url>
    <inceptionYear>2024</inceptionYear>
    
    <licenses>
        <license>
            <name>Apache License, Version 2.0</name>
            <url>https://www.apache.org/licenses/LICENSE-2.0</url>
            <distribution>repo</distribution>
        </license>
    </licenses>
    
    <developers>
        <developer>
            <id>CH</id>
            <name>CH</name>
            <email>your-email@example.com</email>
            <roles>
                <role>architect</role>
                <role>developer</role>
            </roles>
            <timezone>Asia/Shanghai</timezone>
