    <!-- 子模块列表 -->
    <modules>
        <module>spring-support-datasource-starter</module>
        <module>spring-support-common-starter</module>          <!-- 通用功能和工具类 -->
        <module>spring-support-mybatis-starter</module>         <!-- MyBatis Plus集成 -->

        <!-- 认证和安全模块 -->
        <module>spring-support-oauth-client-starter</module>

        <!-- 缓存和存储模块 -->
        <module>spring-support-redis-starter</module>           <!-- Redis缓存集成 -->
        <module>spring-support-filesystem-minio-starter</module>  <!-- MinIO对象存储 -->
        <module>spring-support-filesystem-starter</module>      <!-- 统一文件存储抽象层 -->

        <!-- API文档和接口模块 -->
        <module>spring-support-swagger-starter</module>         <!-- Swagger API文档 -->

        <!-- 消息与队列抽象 -->
        <module>spring-support-queue-starter</module>
        <module>spring-support-queue-mqtt-starter</module>
        <module>spring-support-queue-kafka-starter</module>
        <module>spring-support-queue-rabbitmq-starter</module>
        <module>spring-support-queue-rocketmq-starter</module>

        <!-- Socket 通信模块系列 -->
        <module>spring-support-socket-starter</module>          <!-- Socket 基础抽象层 -->
        <module>spring-support-socket-websocket-starter</module><!-- WebSocket 实现（同端口） -->
        <module>spring-support-socket-io-starter</module>        <!-- Socket.IO 实现 -->
        <module>spring-support-socket-rsocket-starter</module>         <!-- RSocket 实现 -->
        <module>spring-support-socket-sse-starter</module>             <!-- SSE 实现 -->
        <module>spring-support-rpc-starter</module>             <!-- RPC远程调用 -->
        <module>spring-support-sync-starter</module>

        <!-- 搜索和数据处理模块 -->
        <module>spring-support-elasticsearch-starter</module>   <!-- Elasticsearch搜索引擎 -->

        <module>spring-support-discovery-starter</module>       <!-- 服务发现 -->
        <module>spring-support-configcenter-starter</module>    <!-- 配置中心 -->

