        <!-- API文档和接口模块 -->
        <module>spring-support-swagger-starter</module>         <!-- Swagger API文档 -->

        <!-- 消息与队列抽象 -->
        <module>spring-support-queue-starter</module>
        <module>spring-support-queue-mqtt-starter</module>
        <module>spring-support-queue-kafka-starter</module>
        <module>spring-support-queue-rabbitmq-starter</module>
