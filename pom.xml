                    </compilerArgs>
                </configuration>
            </plugin>

            <!-- Maven鍙戝竷鎻掍欢 - 绠＄悊椤圭洰鐗堟湰鍙戝竷 -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-release-plugin</artifactId>
                <version>3.0.1</version>
                <configuration>
                    <tagNameFormat>v@{project.version}</tagNameFormat>  <!-- Git鏍囩鏍煎紡 -->
                    <autoVersionSubmodules>true</autoVersionSubmodules> <!-- 鑷姩鏇存柊瀛愭ā鍧楃増鏈?-->
                </configuration>
            </plugin>

            <!-- Maven鐗堟湰绠＄悊鎻掍欢 - 绠＄悊渚濊禆鐗堟湰 -->
