        <maven-source-plugin.version>3.3.0</maven-source-plugin.version>
        <maven-surefire-plugin.version>3.2.5</maven-surefire-plugin.version>
        <maven-javadoc-plugin.version>3.6.3</maven-javadoc-plugin.version>
        <maven-release-plugin.version>3.0.1</maven-release-plugin.version>
        <versions-maven-plugin.version>2.16.2</versions-maven-plugin.version>
        <flatten-maven-plugin.version>1.6.0</flatten-maven-plugin.version>
        <maven-enforcer-plugin.version>3.4.1</maven-enforcer-plugin.version>
        
        <!-- 构建控制属性 -->
        <skipTests>true</skipTests>  <!-- 默认跳过测试，可通过 mvn clean install -DskipTests=false 执行测试 -->
        
        <!-- Maven仓库配置 - 可通过profile或-D参数覆盖 -->
        <maven.repo.releases.id>maven-releases</maven.repo.releases.id>
        <maven.repo.releases.url>http://localhost:8081/repository/maven-releases/</maven.repo.releases.url>
        <maven.repo.snapshots.id>maven-snapshots</maven.repo.snapshots.id>
        <maven.repo.snapshots.url>http://localhost:8081/repository/maven-snapshots/</maven.repo.snapshots.url>
    </properties>

