        <!-- Socket 通信模块系列 -->
        <module>spring-support-socket-starter</module>          <!-- Socket 基础抽象层 -->
        <module>spring-support-socket-websocket-starter</module><!-- WebSocket 实现（同端口） -->
        <module>spring-support-socket-io-starter</module>        <!-- Socket.IO 实现 -->
        <module>spring-support-socket-rsocket-starter</module>         <!-- RSocket 实现 -->
        <module>spring-support-socket-sse-starter</module>             <!-- SSE 实现 -->
