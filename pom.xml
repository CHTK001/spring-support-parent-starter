        <dubbo.version>3.1.1</dubbo.version>                <!-- Dubbo RPC框架版本 -->
        <resilience4j.version>2.2.0</resilience4j.version>  <!-- Resilience4j熔断降级框架版本 -->
        <p6spy.version>3.9.0</p6spy.version>                <!-- P6Spy SQL监控版本 -->
        <easy-captcha.version>1.6.2</easy-captcha.version>  <!-- 验证码生成器版本 -->
        <jackson.version>2.20.0</jackson.version>           <!-- Jackson版本 -->
        <commons-io.version>2.16.1</commons-io.version>     <!-- Commons IO版本 -->
